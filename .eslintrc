{
    "extends": ["eslint:recommended", "react-app", "plugin:storybook/recommended"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020,
        "ecmaFeatures": { "jsx": true }
    },
    "rules": {
        "react/prop-types": "off",
        "no-unused-vars": "warn",
        "no-prototype-builtins": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": ["react"]
}
